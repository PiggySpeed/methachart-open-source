{
  "name": "MethaChart",
  "version": "1.2.0",
  "description": "generates methadone log forms",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "production": "webpack -p && electron .",
    "watch": "webpack-dev-server --hot --watch --inline --progress",
    "test": "mocha ./src/test --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "build": "electron-packager . MethaChart --platform=win32 --arch=x64 --version=1.2.5 --overwrite --ignore=build --ignore=node_modules --prune --asar=true --out=./build"
  },
  "author": "John Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PiggySpeed/methachart.git"
  },
  "bugs": {
    "url": "https://github.com/PiggySpeed/methachart/issues"
  },
  "dependencies": {
    "less": "^2.7.1",
    "material-ui": "^0.16.4",
    "moment": "^2.17.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "electron": "^1.4.10",
    "electron-packager": "^8.3.0",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "path": "^0.12.7",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
