{
  "name": "MethaChart",
  "version": "1.2.0",
  "description": "generates methadone log forms",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "production": "webpack -p && electron .",
    "watch": "webpack-dev-server --hot --watch --inline --progress",
    "test": "mocha ./src/test --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "build": "electron-packager . MethaChart --platform=win32 --arch=x64 --version=1.2.5 --overwrite --ignore=build --ignore=node_modules --prune --asar=true --out=./build"
  },
  "author": "John Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PiggySpeed/methachart/issues"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "less": "^2.7.1",
    "material-ui": "^0.15.1",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-webpack-loaders": "^0.3.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "^1.2.5",
    "express": "^4.14.0",
    "history": "^2.0.0",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "path": "^0.12.7",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
